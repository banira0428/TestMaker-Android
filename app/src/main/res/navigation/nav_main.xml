<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/page_home">

    <action
        android:id="@+id/action_global_page_home"
        app:destination="@id/page_home" />

    <action
        android:id="@+id/action_global_page_group"
        app:destination="@id/page_group" />

    <fragment
        android:id="@id/page_home"
        android:name="jp.gr.java_conf.foobar.testmaker.service.view.main.HomeFragment"
        android:label="@string/app_name"
        >
    </fragment>
    <fragment
        android:id="@id/page_search"
        android:name="jp.gr.java_conf.foobar.testmaker.service.view.online.PublishedWorkbookListFragment"
        android:label="@string/title_activity_public_tests"
        tools:layout="@layout/fragment_group_list">
    </fragment>
    <fragment
        android:id="@id/page_group"
        android:name="jp.gr.java_conf.foobar.testmaker.service.view.group.GroupContainerFragment"
        android:label="@string/title_page_group"
        tools:layout="@layout/fragment_group_list">
    </fragment>
    <fragment
        android:id="@id/page_settings"
        android:name="jp.gr.java_conf.foobar.testmaker.service.view.preference.SettingsContainerFragment"
        android:label="@string/title_page_settings"
        tools:layout="@layout/fragment_group_list">
    </fragment>

</navigation>